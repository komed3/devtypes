{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [ "./types/*" ],
  "exclude" : [ "./types/index.d.ts" ],
  "entryPointStrategy": "resolve",
  "out": "docs",
  "githubPages": true,
  "searchInComments": true,
  "searchInDocuments": true,
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "customFooterHtml": "<b>devtypes v2.0</b> — Copyright 2025–2026 Paul Köhler (komed3) <br /> Distributed under the MIT license.",
  "hideGenerator": true,
  "headings": {
    "readme": false,
    "document": false
  },
  "plugin": [ "typedoc-github-theme" ],
  "theme": "typedoc-github-theme"
}
